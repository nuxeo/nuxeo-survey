<?xml version="1.0"?>
<component name="org.nuxeo.ecm.survey.lifecycle"
  version="1.0">

  <require>org.nuxeo.ecm.platform.routing.lifecycle.contrib</require>


  <extension target="org.nuxeo.ecm.core.lifecycle.LifeCycleService"
    point="types">
    <types>
      <type name="Survey" noRecursionForTransitions="toValidate,toReady,toRunning,toDone,toCanceled">
        documentRouteElement
      </type>
      <type name="BranchedQuestionContainer" noRecursionForTransitions="toValidated,toReady,toRunning,toDone,toCanceled">
        documentRouteElement
      </type>
      <type name="Question">documentRouteElement</type>
    </types>
  </extension>
</component>